{
  "name": "@flinbein/xjmapper",
  "version": "2.1.0",
  "keywords": [
    "serialization"
  ],
  "homepage": "https://github.com/flinbein/xjmapper#readme",
  "bugs": {
    "url": "https://github.com/flinbein/xjmapper/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flinbein/xjmapper.git"
  },
  "license": "MIT",
  "author": "dpohvar@gmail.com",
  "type": "module",
  "module": "index.mjs",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build:esm": "esbuild src/index.ts --bundle --minify --sourcemap --format=esm --outfile=index.mjs",
    "build:cjs": "esbuild src/index.ts --bundle --minify --sourcemap --format=cjs --outfile=index.js",
    "build:types": "tsc -b ./tsconfig.types.json",
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "esbuild": "^0.25.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.2"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "index.js.map",
    "index.mjs.map"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js",
      "default": "./index.js"
    }
  }
}
