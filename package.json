{
  "name": "xjmapper",
  "version": "1.0.6",
  "type": "module",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "typings": "./dist/types/index.d.ts",
  "author": "dpohvar@gmail.com",
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run compile",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  }
}
